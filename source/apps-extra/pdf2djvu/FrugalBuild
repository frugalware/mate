options+=('asneeded')

pkgname=pdf2djvu
pkgver=0.7.19
pkgrel=1
pkgdesc="Creates DjVu files from PDF files."
depends=('djvulibre' 'poppler>=0.24.1' 'libgomp' 'bzip2')
groups=('apps-extra')
archs=('i686' 'x86_64')
_F_googlecode_sep="_"
Finclude googlecode
_F_cd_path="$pkgname-$pkgver"
source=(https://bitbucket.org/jwilk/pdf2djvu/downloads/$pkgname-$pkgver.tar.xz)
sha1sums=('588400265430b912dba56f7fd38163ceae693d2f')

# optimization OK
