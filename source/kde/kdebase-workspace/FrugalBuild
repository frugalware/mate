########### TOP_SRC_DIR ####################
## plasma ->
##	desktop ->
##		-- applets/*
##		-- containments/*
##		-- runners/*
##		-- shell/*
##
## plasma ->
##	generic ->
##		-- animators/*
##		-- applets/*
##		-- containmentactions/*
##		-- dataengines/*
##		-- runners/*
##		-- scriptengines/*
##		-- shells/*
##		-- wallpapers/*
## plasma ->
##	netbook ->
##		-- applets/*
##		-- containments/*
##		-- dataengines/*
##		-- desktoptheme/*
##		-- shell/*
## plasma ->
##	screensaver ->
##		-- containments/*
##		-- shell/*
## plasma ->
##	tools ->
##		-- desktopthemedetails/*
##		-- engineexplorer/*
##		-- wallpaperviewer/*
##
#################################################


