options+=('asneeded')

pkgname=libpwquality
pkgver=1.2.4
pkgrel=1
pkgdesc="A library for password generation and password quality checking"
url="https://fedorahosted.org/libpwquality/"
archs=('i686' 'x86_64')
groups=('lib')
depends=('cracklib' 'pam')
up2date="Flasttar https://fedorahosted.org/releases/l/i/$pkgname/"
source="https://fedorahosted.org/releases/l/i/$pkgname/$pkgname-$pkgver.tar.bz2"
sha1sums=('ba5d03319ab3e227d7f236d0ecbd0ec09f4be1d6')
Fconfopts="$Fconfopts --prefix=/usr --with-securedir=/usr/lib/security"
# optimization OK
