options+=('asneeded')

pkgname=libwebp
pkgver=0.3.1
pkgrel=1
pkgdesc="A library for a new image format comparable to JPEG."
depends=('libpng' 'libjpeg')
groups=('lib')
archs=('i686' 'x86_64')
_F_github_name="$pkgname"
_F_github_author="webmproject"
_F_github_tag=y
_F_github_ver="v${pkgver}"
Finclude github
up2date="$up2date | sed 's/v//'"
sha1sums=('52e3d2b6c0b80319baa33b8ebed89618769d9dd8')

# optimization OK
