options+=('asneeded')

pkgname=libmp3splt
pkgver=0.9.1
pkgextraver=a
pkgrel=2
pkgdesc="Library for mp3splt a utility for mp3/ogg splitting without decoding"
depends=('pcre' 'libtool' 'libvorbis' 'libmad' 'libid3tag')
_F_sourceforge_dirname="mp3splt"
_F_sourceforge_rss_limit="180"
Finclude sourceforge
up2date="$up2date | sed "s/$pkgextraver//""
groups=('multimedia-extra')
archs=('i686' 'x86_64')
sha1sums=('189ecef22274d599f8503e77da26c7a27264d740')


# optimization OK
