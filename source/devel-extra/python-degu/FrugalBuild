pkgname=python-degu
pkgver=0.7.0
pkgrel=1
pkgdesc="An embedded HTTP server and client library for Python3"
archs=('i686' 'x86_64')
groups=('devel-extra')
depends=('python-3.0>=3.4.1')
_F_launchpad_name=degu
_F_launchpad_subdir="0.7"
Finclude launchpad
sha1sums=('ded300de79d6f815cb35e7b44c362f0de699f192')

build()
{
	Fcd
	python3 setup.py install --prefix=/usr --root=$Fdestdir || Fdie
}

# optimization OK
